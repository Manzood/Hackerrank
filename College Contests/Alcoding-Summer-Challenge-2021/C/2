#include "bits/stdc++.h"
using namespace std;
#define debug(x) cout << #x << " = " << x << endl;
#define int long long

void insertionSort(int arr[], int n)
{
    for (int i = 1; i < n; i++) {
        int j = i - 1;
        while (j >= 0 && arr[j] > arr[i]) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = arr[i];
    }
}

int32_t main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        scanf("%lld", &n);
        vector <int> a(n);
        for (int i = 0; i < n; i++) {
            scanf("%lld", &a[i]);
        }
        // one from the left, one from the right
        vector <pair <int, int>> b(n);
        for (int i = 0; i < n; i++) {
            b[i].first = a[i];
            b[i].second = i;
        }
        sort (b.begin(), b.end());
        for (int i = 0; i < n; i++) {
            a[b[i].second] = i + 1;
        }
        // reordered everything
        int ans = 0;
        int r = 0;
        for (int i = 0; i < n; i++) {
            if (a[i] > ans) {
                if (a[i] - ans + r - 1 > 0) {
                    if (a[i] - ans + r - 1 > 1) {
                        r++;
                    } else {
                        ans++;
                    }
                }
            }
        }
    }
}
